#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x82bcb48 .scope module, "tff_tb" "tff_tb" 2 3;
 .timescale 0 0;
v0x82ba1f8_0 .var "clk", 0 0;
v0x82ba278_0 .net "q", 0 0, v0x82ba0d0_0; 1 drivers
v0x82ba2e8_0 .var "reset", 0 0;
v0x82ba358_0 .var "t", 0 0;
S_0x82bcbc8 .scope module, "tff_0" "tff" 2 7, 3 1, S_0x82bcb48;
 .timescale 0 0;
v0x82bcc48_0 .net "clk", 0 0, v0x82ba1f8_0; 1 drivers
v0x82ba0d0_0 .var "q", 0 0;
v0x82ba130_0 .net "reset", 0 0, v0x82ba2e8_0; 1 drivers
v0x82ba190_0 .net "t", 0 0, v0x82ba358_0; 1 drivers
E_0x82bc760 .event posedge, v0x82bcc48_0;
    .scope S_0x82bcbc8;
T_0 ;
    %wait E_0x82bc760;
    %load/v 8, v0x82ba130_0, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v0x82ba0d0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0x82ba0d0_0, 1;
    %load/v 9, v0x82ba190_0, 1;
    %xor 8, 9, 1;
    %set/v v0x82ba0d0_0, 8, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x82bcb48;
T_1 ;
    %set/v v0x82ba2e8_0, 1, 1;
    %set/v v0x82ba1f8_0, 0, 1;
    %set/v v0x82ba358_0, 0, 1;
    %delay 4, 0;
    %set/v v0x82ba2e8_0, 0, 1;
    %set/v v0x82ba358_0, 1, 1;
    %delay 2, 0;
    %set/v v0x82ba358_0, 1, 1;
    %delay 2, 0;
    %set/v v0x82ba358_0, 0, 1;
    %delay 4, 0;
    %set/v v0x82ba358_0, 1, 1;
    %delay 8, 0;
    %vpi_call 2 25 "$finish";
    %end;
    .thread T_1;
    .scope S_0x82bcb48;
T_2 ;
    %delay 2, 0;
    %load/v 8, v0x82ba1f8_0, 1;
    %inv 8, 1;
    %set/v v0x82ba1f8_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x82bcb48;
T_3 ;
    %vpi_call 2 32 "$monitor", $time, ") clk = %b, reset = %b, t = %b, q = %b", v0x82ba1f8_0, v0x82ba2e8_0, v0x82ba358_0, v0x82ba278_0;
    %end;
    .thread T_3;
    .scope S_0x82bcb48;
T_4 ;
    %vpi_call 2 36 "$dumpfile", "tff_test.vcd";
    %vpi_call 2 37 "$dumpvars";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tff_tb.v";
    "./tff.v";
