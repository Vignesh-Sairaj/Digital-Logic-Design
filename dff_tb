#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xa1fdae8 .scope module, "dff_tb" "dff_tb" 2 3;
 .timescale 0 0;
v0xa1fb1c8_0 .var "clk", 0 0;
v0xa1fb248_0 .var "d", 0 0;
v0xa1fb2b8_0 .net "q", 0 0, v0xa1fb100_0; 1 drivers
v0xa1fb328_0 .var "reset", 0 0;
S_0xa1fdb68 .scope module, "dff_0" "dff" 2 7, 3 1, S_0xa1fdae8;
 .timescale 0 0;
v0xa1fdbe8_0 .net "clk", 0 0, v0xa1fb1c8_0; 1 drivers
v0xa1fb0a0_0 .net "d", 0 0, v0xa1fb248_0; 1 drivers
v0xa1fb100_0 .var "q", 0 0;
v0xa1fb160_0 .net "reset", 0 0, v0xa1fb328_0; 1 drivers
E_0xa1fd650 .event posedge, v0xa1fdbe8_0;
    .scope S_0xa1fdb68;
T_0 ;
    %wait E_0xa1fd650;
    %load/v 8, v0xa1fb160_0, 1;
    %jmp/0xz  T_0.0, 8;
    %set/v v0xa1fb100_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0xa1fb0a0_0, 1;
    %set/v v0xa1fb100_0, 8, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xa1fdae8;
T_1 ;
    %set/v v0xa1fb328_0, 1, 1;
    %set/v v0xa1fb1c8_0, 0, 1;
    %set/v v0xa1fb248_0, 0, 1;
    %delay 4, 0;
    %set/v v0xa1fb328_0, 0, 1;
    %set/v v0xa1fb248_0, 1, 1;
    %delay 2, 0;
    %set/v v0xa1fb248_0, 1, 1;
    %delay 4, 0;
    %set/v v0xa1fb248_0, 0, 1;
    %delay 2, 0;
    %set/v v0xa1fb248_0, 1, 1;
    %delay 8, 0;
    %vpi_call 2 25 "$finish";
    %end;
    .thread T_1;
    .scope S_0xa1fdae8;
T_2 ;
    %delay 2, 0;
    %load/v 8, v0xa1fb1c8_0, 1;
    %inv 8, 1;
    %set/v v0xa1fb1c8_0, 8, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0xa1fdae8;
T_3 ;
    %vpi_call 2 32 "$monitor", $time, ") clk = %b, reset = %b, d = %b, q = %b", v0xa1fb1c8_0, v0xa1fb328_0, v0xa1fb248_0, v0xa1fb2b8_0;
    %end;
    .thread T_3;
    .scope S_0xa1fdae8;
T_4 ;
    %vpi_call 2 36 "$dumpfile", "dff_test.vcd";
    %vpi_call 2 37 "$dumpvars";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "dff_tb.v";
    "./dff.v";
